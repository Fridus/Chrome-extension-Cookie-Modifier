{
  "manifest_version": 2,

  "name" :"Cookie test",
  "version" :"0.0.1",
  "description" :"Le truc du tutoriel",

  "icons": { "128": "icon_big.png" },
  "options_page":"page_options.html",

  "permissions": [
    "tabs",
    "cookies",
    "<all_urls>",
    "storage"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}